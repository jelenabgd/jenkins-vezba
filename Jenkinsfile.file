pipeline {
    agent any

    stages {
        stage('Deploy') {
            when {
              expression {
                currentBuild.result == null || currentBuild.result == 'SUCCESS' 
              }
            }
            steps {
                sh 'echo "currentBuild.number"'
                sh '''IME_GRANE=${env.BRANCH_NAME}
                    echo IME_GRANE'''
            }
        }
    }
}