pipeline {
    agent any

    stages {
        stage('Deploy') {
            when {
              expression {
                currentBuild.result == null || currentBuild.result == 'SUCCESS' 
              }
            }
            steps {
                sh 'echo "currentBuild.number"'
                sh 'BUILD_NUMBER=${env.BUILD_NUMBER}'
                sh 'BUILD_NUMBER=${env.currentBuild.number}'
            }
        }
    }
}